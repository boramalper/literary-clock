{
  "manifest_version": 2,
  "name": "Literary Clock",
  "version": "1.0",

  "description": "Read time in quotations.",

  "icons": {
    "48": "icons/icon48.png"
  },

  "permissions": [
    "http://himawari8.nict.go.jp/img/D531106/*",
    "http://himawari8-dl.nict.go.jp/himawari8/img/D531106/latest.json",
    "storage",
    "unlimitedStorage"
  ],

  "background": {
    "page": "src/background.html"
  },

  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  }
}