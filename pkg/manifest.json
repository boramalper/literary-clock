{
  "manifest_version": 2,
  "name": "Literary Clock",
  "version": "1.0.0",

  "description": "Read time in quotations.",

  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },

  "permissions": [
    "https://www.data.jma.go.jp/mscweb/data/himawari/img/fd_/",
    "storage",
    "unlimitedStorage"
  ],

  "background": {
    "page": "src/background.html"
  },

  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  }
}